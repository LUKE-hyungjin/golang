# Banking System Environment Variables

# Application
BANK_APP_ENVIRONMENT=development
BANK_APP_MODE=debug

# Server
BANK_SERVER_PORT=8080

# Database
BANK_DATABASE_DSN=banking.db
BANK_DATABASE_MAX_OPEN_CONNS=25

# Security
BANK_SECURITY_JWT_SECRET=your-secret-key-here
BANK_SECURITY_RATE_LIMIT=100

# Logging
BANK_LOGGING_LEVEL=debug
BANK_LOGGING_FORMAT=json

# Features
BANK_FEATURES_ENABLE_METRICS=true
BANK_FEATURES_ENABLE_PROFILING=false
BANK_FEATURES_MAINTENANCE_MODE=false